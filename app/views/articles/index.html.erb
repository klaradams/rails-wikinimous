<div class="container">
  <h1>Wikinonomous</h1>

  <ul>
    <% @articles.each do |article| %>
    <li><%=link_to article.title, article_path(article),
      class: "titles"%></li>
    <% end %>
  </ul>
</div>

<div class="buttons">
  <%= link_to "Create a new article", "articles/new",
    class: "btn"%>
</div>
